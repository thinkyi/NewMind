@using ThinkYi.Domain
@model List<I18N>
<div id="header">
    <div class="top">
        <div class="langbox"><img src="~/Content/images/txt.jpg" width="155" height="20" class="img_txt" /> English  |  繁体</div>
        <div class="telbox"><span class="tel">0769-3321 8944</span></div>
        <div class="searchbox"><input type="text" value="请输入关键字" class="input1" /><input type="button" value="搜索" class="input2" /></div>
    </div>
    <div class="menu">
        <ul>
            @{
                int count = Model.Count;
                for (int i = 0; i < count; i++)
                {
                    <li><a href="#">@Model[i].Name</a></li>
                    if (i < count - 1)
                    {
                        <li class="menu_fg">|</li>
                    }
                }
            }
        </ul>
    </div>
</div>